<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <Button
        android:id="@+id/start_test_activity_1"
        android:text="startTestActivity_1"
        android:paddingLeft="40dp"
        android:drawableLeft="@drawable/__leak_canary_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <tools.haha.com.androidtools.ui.MyCustomView_1
        android:id="@+id/my_custom_view_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="300dp">

        <ViewFlipper
            android:id="@+id/flipper"
            android:layout_width="200dp"
            android:layout_height="200dp">
        </ViewFlipper>

        <Button
            android:text="flip"
            android:id="@+id/switch_page_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="300dp">

        <FrameLayout
            android:id="@+id/page_container"
            android:layout_width="200dp"
            android:layout_height="200dp">
        </FrameLayout>

        <Button
            android:text="changePage"
            android:id="@+id/switch_page_button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <Button
            android:id="@+id/cancel"
            android:text="cancel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>

</LinearLayout>